interface vlan203
  description SH_203_L3
  ip address 172.19.21.1 255.255.255.240
 ip ospf authentication message-digest
 ip ospf message-digest-key 1 md5 7 blah
  ip ospf network broadcast
  ip ospf cost 100
  ip pim sparse-mode
 no shut
!
